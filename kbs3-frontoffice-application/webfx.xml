<?xml version="1.0" encoding="UTF-8"?>
<project applicationId="org.kadampabookings.kbs.frontoffice" applicationLabel="Kadampa bookings">

    <exported-packages>
        <source-packages/>
    </exported-packages>

    <providers>
        <provider interface="javafx.application.Application">org.kadampabookings.kbs.frontoffice.KBSFrontOfficeApplication</provider>
    </providers>

    <dependencies>
        <used-by-source-modules/>

        <!-- Allow configuration variables from environment in Java executables (OpenJFX or Gluon) -->
        <plugin-module executable-target="java">webfx-stack-conf-env-java</plugin-module>

        <!-- Event activities -->
        <plugin-module>modality-base-frontoffice-activities-home</plugin-module>
        <plugin-module>modality-event-frontoffice-activities-booking</plugin-module>
        <plugin-module>modality-base-frontoffice-activities-alerts</plugin-module>
        <plugin-module>modality-base-frontoffice-activities-account</plugin-module>
    </dependencies>


</project>